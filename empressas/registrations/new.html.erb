<br>
   <div class="container" >
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel"><%= link_to '', home_path,:title => 'Fechar', :method => :get , :class =>"btn btn-danger btn3d fa fa-times pull-right"%> 
                    <div class="panel-heading">
                        <h3 class="panel-title" style="color:#fff">Registrar Empresa</h3>
                    </div>
                    <div class="panel-body">

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
     <div class="form-group">

 <div class="field">
    <%= f.label :empresa,"CNPJ", class: "control-label"%><br />
    <%= f.text_field :empresa, class: "form-control" %>
  </div>

  <br>

 <div class="field">
    <%= f.label :razao_social,"Razão Social", class: "control-label"%><br />
    <%= f.text_field :razao_social, class: "form-control" %>
  </div>

  <br>

  <div class="field">
    <%= f.label :email,"E-mail", class: "control-label"%><br />
    <%= f.email_field :email, class: "form-control" %>
  </div>

  <br>

  <div class="field">
    <%= f.label :password,"Senha", class: "control-label" %>
    <% if @minimum_password_length %>
    <em>(mínimo de <%= @minimum_password_length %> caracteres)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", class: "form-control"  %>
  </div>

  <br>

  <div class="field">
    <%= f.label :password_confirmation,"Confirmação de Senha", class: "control-label" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control"  %>
  </div>
  </div>

   <div  class="form-groups actions">
    <%= f.submit "Registrar" , class: "btn btn-success btn-block" %>
  </div>

<% end %>
<br />
<%= render "empressas/shared/links" %>
                   </div>
                </div>
            </div>
        </div>
    </div>
    